<% require 'pry' %>
<% binding.pry %>

<%= form_for [@pin, @comment] do |f| %>
	<div class='field'> 
		<h5> <%= f.label "Title for your comment" %></h5> 
    
	</div>

	<div class='field'>
		<h5><%= f.label "Leave a comment here" %></h5>
	</div>

	<div class='action field'>
		<%= f.submit %>
	</div>


<% end %> 