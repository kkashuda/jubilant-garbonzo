 <tbody>
    <div class="transitions-enabled" id="pins">
      <% current_user.pins.each do |pin| %> 
        <div class="box panel panel-default">
          <%= link_to (image_tag pin.image.url), pin %>
          <h2>
            <%= link_to pin.title, pin %>
          </h2>
        </div>  
      <% end %> 
    </div>
  </tbody>